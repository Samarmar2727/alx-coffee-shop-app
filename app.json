{
  "expo": {
    "name": "alx-coffee-shop-app",
    "slug": "alx-coffee-shop-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "alxcoffeeshopapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "permissions": ["ACCESS_FINE_LOCATION"]
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
